<h1>Listing Musicians</h1>

<table>
  <tr>
    <th>Name</th>
    <th>email</th>
  </tr>

<% @musicians.each do |sample| %>
  <tr>
  
<td><%= sample.email %></td>
    <td><%= link_to 'Show', sample %></td>
    <td><%= link_to 'Edit', edit_musician_path(sample) %></td>
    <td><%= link_to 'Destroy', sample, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Musician', new_musician_path %>
